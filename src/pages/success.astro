---
import Layout from '../layouts/Layout.astro';
import SuccessPaid from '../components/SuccessPaid';

const title = "Successfully Paid"
const description = "You have successfully placed an order. Create an account to save your order to your account."
---

<Layout title={title} description={description}>
	<SuccessPaid client:only />
</Layout>

<script>
	import { initVisit } from '../stores/visit';
	import { getLatestOrder } from '../stores/order';

	initVisit();

	getLatestOrder();

</script>